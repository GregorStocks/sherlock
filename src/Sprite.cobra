class SpriteId
	var id as int

	cue init(id as int)
		base.init
		.id = id
	
	def getHashCode as int is override
		return .id

	def equals(o as Object?) as bool is override
		return o inherits SpriteId and .id == (o to SpriteId).id

class Sprite
	var x as int
	var y as int

	cue init(x as int, y as int)
		base.init
		.x = x
		.y = y
